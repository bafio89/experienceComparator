{
  "name": "spring-boot-starter-react",
  "license": "MIT",
  "scripts": {
    "bundle": "parcel build src/main/js/index.js --no-cache --out-dir target/classes/static/dist --out-file bundle --public-url /dist",
    "postbundle": "date-now --format='DD.MM.YYYY-HH:mm:ss (A)' | cowthink $1",
    "watch": "parcel watch src/main/js/index.js --out-dir target/classes/static/dist --out-file bundle --public-url /dist",
    "postwatch": "date-now --format='DD.MM.YYYY-HH:mm:ss (A)' | cowthink $1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0-14",
    "@fortawesome/free-solid-svg-icons": "^5.1.0-11",
    "@fortawesome/react-fontawesome": "0.1.0-11",
    "aphrodite": "^2.2.2",
    "axios": "^0.18.0",
    "i": "^0.3.6",
    "npm": "^6.2.0",
    "parcel-bundler": "^1.9.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  },
  "postcss": {
    "modules": false,
    "plugins": {
      "autoprefixer": {
        "browsers": [
          ">5%",
          "last 4 versions",
          "Firefox ESR",
          "not ie < 11"
        ],
        "flexbox": "no-2009"
      }
    }
  },
  "devDependencies": {
    "autoprefixer": "^7.2.1",
    "babel-preset-react": "^6.24.1",
    "cowsay": "^1.3.1",
    "cross-env": "^5.1.1",
    "date-now-cli": "^1.1.0",
    "node-sass": "^4.9.2"
  }
}
